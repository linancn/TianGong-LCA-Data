<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://lca.jrc.it/ILCD/LCIAMethodologies"
           xmlns:common="http://lca.jrc.it/ILCD/Common"
           version="1.1"
           targetNamespace="http://lca.jrc.it/ILCD/LCIAMethodologies"
           elementFormDefault="qualified">
   <xs:element name="ILCDLCIAMethodologies">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="methodology"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="methodology">
      <xs:complexType>
         <xs:attribute name="name" use="required"/>
      </xs:complexType>
   </xs:element>
</xs:schema>