			<common:UUID>0212cd93-b75d-47db-966d-4692b225c53b</common:UUID>
			<name>
				<baseName xml:lang="en">Coal-based powdered activated carbon</baseName>
				<baseName xml:lang="zh">煤质活性炭</baseName>
