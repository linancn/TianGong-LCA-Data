			<common:synonyms xml:lang="en">Phenol-formaldehyde resin; Bakelite; Novolac; Resol</common:synonyms>
			<common:synonyms xml:lang="zh">Phenol-formaldehyde resin; Bakelite; Novolac; Resol</common:synonyms>
