			<common:synonyms xml:lang="en">Crude coal</common:synonyms>
			<common:synonyms xml:lang="zh">Crude coal</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
