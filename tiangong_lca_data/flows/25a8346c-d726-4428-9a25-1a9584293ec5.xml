			<name>
				<baseName xml:lang="en">Paraffin</baseName>
				<baseName xml:lang="zh">石蜡</baseName>
			</name>
			<common:synonyms xml:lang="en">Paraffin wax; Liquid paraffin; Alkane; Kerosene; Mineral oil</common:synonyms>
			<common:synonyms xml:lang="zh">Paraffin wax; Liquid paraffin; Alkane; Kerosene; Mineral oil</common:synonyms>
			<classificationInformation>
