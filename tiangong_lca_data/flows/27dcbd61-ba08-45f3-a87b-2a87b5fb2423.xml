				<baseName xml:lang="en">Ferric oxide</baseName>
				<baseName xml:lang="zh">三氧化二铁</baseName>
			</name>
			<common:synonyms xml:lang="en">rust; Ferrum</common:synonyms>
			<classificationInformation>
				<common:classification>
					<common:category level="0">Materials production</common:category>
					<common:category level="1">Inorganic chemicals</common:category>
