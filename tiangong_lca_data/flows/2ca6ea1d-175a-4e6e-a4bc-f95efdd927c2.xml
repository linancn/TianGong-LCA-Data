			<name>
				<baseName xml:lang="en">Sulfuric acid(98%)</baseName>
				<baseName xml:lang="zh">硫酸(98%)</baseName>
			</name>
			<common:synonyms xml:lang="en">Hydrogen sulfate; SULFURIC ACID REAGENT GRADE 95-98%; So42-; Sulfuric acid 96 %; Acide sulfurique; So4--; Acid Mist; Sulfur acid; Dipping acid; Inorganic acid</common:synonyms>
			<common:synonyms xml:lang="zh">Hydrogen sulfate; SULFURIC ACID REAGENT GRADE 95-98%; So42-; Sulfuric acid 96 %; Acide sulfurique; So4--; Acid Mist; Sulfur acid; Dipping acid; Inorganic acid</common:synonyms>
			<classificationInformation>
