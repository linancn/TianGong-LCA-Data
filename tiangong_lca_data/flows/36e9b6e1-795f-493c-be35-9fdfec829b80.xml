			<name>
				<baseName xml:lang="en">N-Phosphonomethyl aminodiacetic acid</baseName>
				<baseName xml:lang="zh">N-膦酰甲基氨基二乙酸</baseName>
			</name>
			<common:synonyms xml:lang="en">N-(Phosphonomethyl)iminodiacetic acid; PMIDA; H 4 pmida; N-(phosphonomethyl)iminodiacetate; pmida 4-</common:synonyms>
			<common:synonyms xml:lang="zh">N-(Phosphonomethyl)iminodiacetic acid; PMIDA; H 4 pmida; N-(phosphonomethyl)iminodiacetate; pmida 4-</common:synonyms>
			<classificationInformation>
