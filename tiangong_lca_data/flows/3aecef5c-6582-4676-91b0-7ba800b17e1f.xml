			<common:synonyms xml:lang="en">Medicago sativa; buffalo herb; Lucerne; purple medic</common:synonyms>
			<common:synonyms xml:lang="zh">Medicago sativa; buffalo herb; Lucerne; purple medic</common:synonyms>
			<classificationInformation>
