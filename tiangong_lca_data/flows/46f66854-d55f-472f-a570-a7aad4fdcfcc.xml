			<common:synonyms xml:lang="en">Abrasive blasting; Sandblasting; Media blasting; Grit blasting; Surface treatment; Surface finishing</common:synonyms>
			<common:synonyms xml:lang="zh">Abrasive blasting; Sandblasting; Media blasting; Grit blasting; Surface treatment; Surface finishing</common:synonyms>
