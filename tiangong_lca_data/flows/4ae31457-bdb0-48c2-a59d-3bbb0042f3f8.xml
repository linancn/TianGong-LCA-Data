			<common:synonyms xml:lang="en">Ethyl Methyl Carbonate; Ethyl Carbonate; Ethoxyacetic acid, methyl ester</common:synonyms>
			<common:synonyms xml:lang="zh">Ethyl Methyl Carbonate; Ethyl Carbonate; Ethoxyacetic acid, methyl ester</common:synonyms>
