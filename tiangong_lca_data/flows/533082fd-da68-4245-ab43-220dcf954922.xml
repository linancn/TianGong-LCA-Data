			<name>
				<baseName xml:lang="en"> Clinker, portland cement clinker, 52.5mpa </baseName>
				<baseName xml:lang="zh">熟料，硅酸盐水泥熟料，52.5MPa</baseName>
			</name>
			<common:synonyms xml:lang="en">Portland cement clinker; cement clinker; 52.5 grade clinker</common:synonyms>
			<common:synonyms xml:lang="zh">Portland cement clinker; cement clinker; 52.5 grade clinker</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
