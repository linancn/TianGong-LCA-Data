			<name>
				<baseName xml:lang="en">N-Methyl-2-pyrrolidone (NMP) </baseName>
				<baseName xml:lang="zh">N-甲基-2-吡咯烷酮 </baseName>
			</name>
			<common:synonyms xml:lang="en">1-Methyl-2-pyrrolidone; N-Methylpyrrolidone; N-Methylpyrrolidinone;Pharmasolve</common:synonyms>
			<common:synonyms xml:lang="zh">1-Methyl-2-pyrrolidone; N-Methylpyrrolidone; N-Methylpyrrolidinone;Pharmasolve</common:synonyms>
			<classificationInformation>
