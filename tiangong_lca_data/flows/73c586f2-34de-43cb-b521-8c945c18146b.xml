			<name>
				<baseName xml:lang="en">Zinc stearate</baseName>
				<baseName xml:lang="zh">硬脂酸锌</baseName>
			</name>
			<common:synonyms xml:lang="en">ZINC STEARATE</common:synonyms>
			<common:synonyms xml:lang="zh">ZINC STEARATE</common:synonyms>
			<classificationInformation>
