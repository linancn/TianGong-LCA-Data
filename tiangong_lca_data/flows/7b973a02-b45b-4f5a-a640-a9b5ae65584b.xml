				<baseName xml:lang="zh">鲍鱼</baseName>
			</name>
			<common:synonyms xml:lang="en">Haliotis; Gastropod mollusk; Sea snail</common:synonyms>
			<common:synonyms xml:lang="zh">Haliotis; Gastropod mollusk; Sea snail</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
