			<common:synonyms xml:lang="en">Native copper; Porphyry copper; Massive deposits; Mixed ores</common:synonyms>
			<common:synonyms xml:lang="zh">Native copper; Porphyry copper; Massive deposits; Mixed ores</common:synonyms>
