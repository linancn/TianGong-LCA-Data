			<common:synonyms xml:lang="en"> soy bean;soya bean;Glycine max</common:synonyms>
			<common:synonyms xml:lang="zh"> soy bean;soya bean;Glycine max</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
