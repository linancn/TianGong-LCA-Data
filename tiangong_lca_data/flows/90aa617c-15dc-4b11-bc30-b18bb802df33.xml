			<name>
				<baseName xml:lang="en">Ethylene carbonate</baseName>
				<baseName xml:lang="zh">碳酸乙烯酯</baseName>
			</name>
			<common:synonyms xml:lang="en">EC; 1,3-dioxolan-2-one; ethylene glycol carbonate</common:synonyms>
			<common:synonyms xml:lang="zh">EC; 1,3-dioxolan-2-one; ethylene glycol carbonate</common:synonyms>
			<classificationInformation>
