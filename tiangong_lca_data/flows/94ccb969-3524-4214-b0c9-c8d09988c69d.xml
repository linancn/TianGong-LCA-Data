			<common:synonyms xml:lang="en">waste battery management system</common:synonyms>
			<common:synonyms xml:lang="zh">waste battery management system</common:synonyms>
			<classificationInformation>
				<common:classification name="ILCD 1.1" classes="../ILCDClassification.xml">
