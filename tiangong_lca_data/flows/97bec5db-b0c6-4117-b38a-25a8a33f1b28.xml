			<common:synonyms xml:lang="en">Ammoniacal nitrogen; NH 3-N; Ammonium; NH 4+</common:synonyms>
			<common:synonyms xml:lang="zh">Ammoniacal nitrogen; NH 3-N; Ammonium; NH 4+</common:synonyms>
