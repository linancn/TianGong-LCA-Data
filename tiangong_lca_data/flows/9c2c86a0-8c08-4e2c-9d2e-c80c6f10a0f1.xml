			<name>
				<baseName xml:lang="en">Silicon-Aluminum Alloy (Si, 12%)</baseName>
				<baseName xml:lang="zh">硅铝合金（Si，12）</baseName>
			</name>
			<common:synonyms xml:lang="en">Si-Al alloy; Aluminum silicon alloy; AlSi12; Silicon aluminum alloy</common:synonyms>
			<common:synonyms xml:lang="zh">Si-Al alloy; Aluminum silicon alloy; AlSi12; Silicon aluminum alloy</common:synonyms>
