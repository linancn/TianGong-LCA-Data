			<name>
				<baseName xml:lang="en">Anhydrous Hydrogen Fluoride</baseName>
				<baseName xml:lang="zh">无水氟化氢</baseName>
			</name>
			<common:synonyms xml:lang="en">Aqueous hydrogen fluoride; HF-A; Hydrofluoric acid</common:synonyms>
			<common:synonyms xml:lang="zh">Aqueous hydrogen fluoride; HF-A; Hydrofluoric acid</common:synonyms>
			<classificationInformation>
