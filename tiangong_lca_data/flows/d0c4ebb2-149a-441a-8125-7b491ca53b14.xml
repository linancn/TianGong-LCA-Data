			<common:synonyms xml:lang="en">Lithium Hexafluorophosphate; Lithium Hexafluorophosphate (1-)</common:synonyms>
			<common:synonyms xml:lang="zh">Lithium Hexafluorophosphate; Lithium Hexafluorophosphate (1-)</common:synonyms>
