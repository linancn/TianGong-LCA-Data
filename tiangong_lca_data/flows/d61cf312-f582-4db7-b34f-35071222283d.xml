			<common:synonyms xml:lang="en">C4；C4+；丁烷;1-丁烯;2-丁烯;异丁烯;丁二烯</common:synonyms>
			<common:synonyms xml:lang="zh">C4；C4+；丁烷;1-丁烯;2-丁烯;异丁烯;丁二烯</common:synonyms>
