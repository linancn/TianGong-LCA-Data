				<baseName xml:lang="zh">裂解汽油</baseName>
			</name>
			<common:synonyms xml:lang="en">Pygas</common:synonyms>
			<common:synonyms xml:lang="zh">Pygas</common:synonyms>
			<classificationInformation>
