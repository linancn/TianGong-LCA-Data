			<common:synonyms xml:lang="en">Aluminum metal powder; Al powder; Aluminium fine powder; Pyro aluminum; Flash powder</common:synonyms>
			<common:synonyms xml:lang="zh">Aluminum metal powder; Al powder; Aluminium fine powder; Pyro aluminum; Flash powder</common:synonyms>
			<classificationInformation>
