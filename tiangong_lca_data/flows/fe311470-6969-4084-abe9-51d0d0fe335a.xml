				<baseName xml:lang="en">Phosphorus Oxychloride</baseName>
				<baseName xml:lang="zh">三氯氧磷</baseName>
			</name>
			<common:synonyms xml:lang="en">Phosphorus chloride; Phosphorus oxytrichloride; Phosphoryl chloride</common:synonyms>
			<common:synonyms xml:lang="zh">Phosphorus chloride; Phosphorus oxytrichloride; Phosphoryl chloride</common:synonyms>
			<classificationInformation>
