	<processInformation>
		<dataSetInformation>
			<common:UUID>135b51f2-7662-4998-bc96-5d1460d57e9b</common:UUID>
			<name>
				<baseName xml:lang="en">Injection well backfilling ; Din=0.15m,L=50m ; Soil in-situ remediation ; Enhanced bioremediation</baseName>
				<baseName xml:lang="zh">注入井回填;Din=0.15m,L=50m;地下水原位修复;生物强化修复</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">Other Services</common:class>
					<common:class level="2">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">The newly built injection wells during the repair process will be backfilled after the repair project is completed.</common:generalComment>
			<common:generalComment xml:lang="zh">将修复过程中新建的注入井，在修复项目结束后进行回填。</common:generalComment>
		</dataSetInformation>
