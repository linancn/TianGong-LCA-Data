	<processInformation>
		<dataSetInformation>
			<common:UUID>2632680c-4718-450c-af7b-b5cf4469f1d4</common:UUID>
			<name>
				<baseName xml:lang="en">Groundwater in-situ remediation ; In-situ Chemical Oxidation ; Pharmaceutical preparation and dispensing equipment</baseName>
				<baseName xml:lang="zh">地下水原位修复;地下水原位化学氧化;药剂制配设备</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Other Services</common:class>
					<common:class level="1">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="zh">制药装置，包括制药储罐、混合装置</common:generalComment>
			<common:generalComment xml:lang="en">Pharmaceutical installations, including pharmaceutical storage tanks, mixing units</common:generalComment>
		</dataSetInformation>
		<quantitativeReference type="Reference flow(s)">
			<referenceToReferenceFlow>3</referenceToReferenceFlow>
