			<name>
				<baseName xml:lang="en">Livestock and poultry breeding ; feed processing stage ; dairy cow scale</baseName>
				<baseName xml:lang="zh">畜禽养殖 ; 饲料加工阶段 ; 奶牛规模化</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
