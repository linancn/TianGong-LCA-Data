			<name>
				<baseName xml:lang="en">Brick production ; concrete pavement brick ; Base blending process</baseName>
				<baseName xml:lang="zh">砖生产 ; 混凝土路面砖 ; 基层混合工艺</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
