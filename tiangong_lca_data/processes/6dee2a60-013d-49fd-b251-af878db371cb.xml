	<processInformation>
		<dataSetInformation>
			<common:UUID>6dee2a60-013d-49fd-b251-af878db371cb</common:UUID>
			<name>
				<baseName xml:lang="en">Groundwater ex-situ remediation ; Pump and treat ; Remediated groundwater</baseName>
				<baseName xml:lang="zh">地下水异位修复;地下水抽出处理;地下水得到修复</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">Other Services</common:class>
					<common:class level="2">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Consumption of electricity to operate extraction wells and wastewater disposal facilities</common:generalComment>
			<common:generalComment xml:lang="zh">消耗电力使抽提井和废水处置设施运行，地下水得到修复</common:generalComment>
		</dataSetInformation>
