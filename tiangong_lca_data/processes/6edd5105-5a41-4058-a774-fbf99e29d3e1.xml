			<name>
				<baseName xml:lang="en">Wet sand  Production;Coarse Crushing;Jaw Crushing Technology</baseName>
				<baseName xml:lang="zh">湿法制砂;砂石粗碎;颚式破碎技术</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
