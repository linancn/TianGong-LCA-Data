	<processInformation>
		<dataSetInformation>
			<common:UUID>77588fb9-34f9-4906-9897-ecfb5aaae88b</common:UUID>
			<name>
				<baseName xml:lang="en">Aluminum electrolytic capacitor production ; components before soaking ; winding</baseName>
				<baseName xml:lang="zh">铝电解电容器生产 ; 浸泡前的元件 ; 绕线</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">Materials production</common:class>
					<common:class level="2">Metals and semimetals</common:class>
