	<processInformation>
		<dataSetInformation>
			<common:UUID>97988c3c-80be-4042-944e-e2e1534bdf49</common:UUID>
			<name>
				<baseName xml:lang="en">cement production;cement, ordinary portland cement, 48.0MPa;mechanical vertical kiln;blast furnace slag</baseName>
				<baseName xml:lang="zh">水泥生产;水泥, 普通硅酸盐水泥, 48.0MPa;机械立窑;炼铁高炉矿渣</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Materials production</common:class>
					<common:class level="1">Other mineralic materials</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">The rotary kiln factory produces cement.</common:generalComment>
			<common:generalComment xml:lang="zh">立窑厂生产水泥。</common:generalComment>
		</dataSetInformation>
		<quantitativeReference type="Reference flow(s)">
			<referenceToReferenceFlow>14</referenceToReferenceFlow>
		</quantitativeReference>
		<time>
			<common:referenceYear>2001</common:referenceYear>
