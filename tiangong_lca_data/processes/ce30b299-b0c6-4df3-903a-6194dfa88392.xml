	<processInformation>
		<dataSetInformation>
			<common:UUID>ce30b299-b0c6-4df3-903a-6194dfa88392</common:UUID>
			<name>
				<baseName xml:lang="en">Soil and groundwater remediation;Ex-situ;Soil washing;Waste water treatment</baseName>
				<baseName xml:lang="zh">土壤和地下水修复;原位;土壤淋洗;废水处理</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">Other Services</common:class>
					<common:class level="2">Repair and maintenance</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Treatment of wastewater using sedimentation tanks, sodium hydroxide, etc.</common:generalComment>
			<common:generalComment xml:lang="zh">使用沉淀池、氢氧化钠等处理废水</common:generalComment>
		</dataSetInformation>
