			<name>
				<baseName xml:lang="en">Remediated soil ; Soil remediation ; Laboratory research ; Microbial electrochemical treatment (MET) ; TPH contaminated soil</baseName>
				<baseName xml:lang="zh">修复后土壤;土壤修复;实验室研究;微生物电化学修复;TPH污染土壤</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Other Services</common:class>
					<common:class level="1">Repair and maintenance</common:class>
				</common:classification>
