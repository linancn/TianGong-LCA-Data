			<dataSelectionAndCombinationPrinciples xml:lang="en">According to a solar module production enterprise in Sichuan production process data</dataSelectionAndCombinationPrinciples>
			<dataSelectionAndCombinationPrinciples xml:lang="zh">根据四川某太阳能电池组件生产企业生产过程的数据资料收集整理</dataSelectionAndCombinationPrinciples>
			<deviationsFromSelectionAndCombinationPrinciples xml:lang="en">None</deviationsFromSelectionAndCombinationPrinciples>
			<deviationsFromSelectionAndCombinationPrinciples xml:lang="zh">无</deviationsFromSelectionAndCombinationPrinciples>
