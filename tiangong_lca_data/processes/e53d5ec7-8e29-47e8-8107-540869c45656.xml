	<processInformation>
		<dataSetInformation>
			<common:UUID>e53d5ec7-8e29-47e8-8107-540869c45656</common:UUID>
			<name>
				<baseName xml:lang="en">The recycled post-magnesia carbon brick ; The recycled post-magnesia carbon brick has a melting point of 2800℃; the billet system is used to prepare ; waste magnesia carbon brick</baseName>
				<baseName xml:lang="zh">后镁碳砖再生生产;再生的后镁碳砖 熔点2800℃;坯体制备;废弃镁碳砖</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">End-of-life treatment</common:class>
					<common:class level="2">Material recycling</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Forming and curing</common:generalComment>
			<common:generalComment xml:lang="zh">成型制坯和养护</common:generalComment>
		</dataSetInformation>
