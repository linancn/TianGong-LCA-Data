			<name>
				<baseName xml:lang="en">Bauxite Mining ; Bauxite ; Cradle</baseName>
				<baseName xml:lang="zh">铝土矿开采｜铝土矿 ; 摇篮</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
