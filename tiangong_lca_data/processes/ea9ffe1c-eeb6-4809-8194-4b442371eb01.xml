	<processInformation>
		<dataSetInformation>
			<common:UUID>ea9ffe1c-eeb6-4809-8194-4b442371eb01</common:UUID>
			<name>
				<baseName xml:lang="en">Post-magnesia carbon brick regeneration production ; recycled post-magnesia carbon brick melting point 2800℃; raw material processing ; waste magnesia carbon brick</baseName>
				<baseName xml:lang="zh">后镁碳砖再生生产;再生的后镁碳砖 熔点2800℃;原料加工;废弃镁碳砖</baseName>
			</name>
			<classificationInformation>
				<common:classification>
					<common:class level="0">Unit processes</common:class>
					<common:class level="1">Materials production</common:class>
					<common:class level="2">Non-energetic raw materials</common:class>
				</common:classification>
			</classificationInformation>
			<common:generalComment xml:lang="en">Production of crushing mixtures and admixtures of raw materials and ingredients</common:generalComment>
			<common:generalComment xml:lang="zh">原料与配料的破碎混料和外加剂的生产</common:generalComment>
		</dataSetInformation>
		<quantitativeReference type="Reference flow(s)">
			<referenceToReferenceFlow>0</referenceToReferenceFlow>
